     map.addSource('route', {\r\n        type: 'geojson',\r\n        data: {\r\n          type: 'Feature',\r\n          properties: {},\r\n          geometry: {\r\n            type: 'LineString',\r\n            coordinates: locations.map((el) => el.coordinates),\r\n          },\r\n        },\r\n      });\r\n\r\n      map.addLayer({\r\n        id: 'route',\r\n        type: 'line',\r\n        source: 'route',\r\n        layout: {\r\n          'line-join': 'round',\r\n          'line-cap': 'round',\r\n        },\r\n        paint: {\r\n          'line-color': 'red',\r\n          'line-width': 3,\r\n          'line-dasharray': [3, 3],\r\n        },\r\n      });\r\n    });\r\n\r\n    map.fitBounds(bounds, {\r\n      padding: {\r\n        top: 200,\r\n        bottom: 150,\r\n        left: 100,\r\n        right: 100,\r\n      },\r\n    });\r\n  } catch (err) {\r\n    console.log('err');\r\n  }\r\n};\r\n"},"lineCount":null}},"error":null,"hash":"7882463bc57bbf3b995242d980718e89","cacheData":{"env":{}}}